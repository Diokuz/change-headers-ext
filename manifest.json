  {
    "name": "Change Response Headers",
    "version": "1.0",
    "description": "Allows to change/add/remove response http headers",
    "manifest_version": 2,
    "permissions": [
        "webRequest",
        "*://*.tcsbank.ru/",
        "*://localhost:*/",
        "webRequestBlocking",
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "background": {
        "scripts": ["main.js"],
        "persistent": true
    },
    "page_action": {
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    }
  }
